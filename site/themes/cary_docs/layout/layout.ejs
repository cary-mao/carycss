<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= page.title %></title>
  <link rel="stylesheet" href="<%= config.root %>vendor/highlight/styles/a11y-dark.css">
  <link rel="stylesheet" href="<%= config.root %>vendor/carycss/index.css">
  <link rel="stylesheet" href="<%= config.root %>cary_docs.css">
</head>
<body>
  <%- body %>
</body>
</html>

<script src="<%= config.root %>vendor/highlight/highlight.pack.js"></script>
<script>
document.addEventListener('DOMContentLoaded', (event) => {
  document.querySelectorAll('pre code').forEach((block) => {
    hljs.highlightBlock(block);
  });
});
</script>
